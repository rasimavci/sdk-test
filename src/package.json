{
  "name": "sdk-test",
  "subdomain": "sdk",
  "version": "1.0.0",
  "description": "KATE  - Kandy Automation Test Environment",
  "main": "sdktest-server.js",
  "scripts": {
    "test": "mocha --verbose",
    "start": "node sdktest-server.js",
    "build": "npm run build:bundle0 && npm run build:bundle2 && npm run build:bundle3",
    "build:bundle0": "browserify calltests.js > bundle0.js",
    "build:bundle2": "browserify client2.js > bundle2.js",
    "build:bundle3": "browserify client3.js > bundle3.js"
  },
  "author": "Rasim Avci",
  "license": "MIT",
  "dependencies": {
    "chai": "^4.1.2",
    "finalhandler": "^0.3.4",
    "node-jet": "git+https://github.com/lipp/node-jet.git",
    "serve-static": "^1.9.2"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^6.4.0",
    "browserify": "^10.2.3",
    "mocha": "^4.0.1"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
